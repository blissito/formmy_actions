{
  "name": "formmy-actions",
  "private": false,
  "version": "1.0.1",
  "description": "Visual AI workflow builder as an embeddable React component",
  "type": "module",
  "main": "./dist/formmy-actions.umd.js",
  "module": "./dist/formmy-actions.es.js",
  "exports": {
    ".": {
      "import": "./dist/formmy-actions.es.js",
      "require": "./dist/formmy-actions.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build",
    "build:all": "npm run build:lib",
    "lint": "eslint .",
    "preview": "vite preview",
    "meta-gen": "tsx src/generator/SelfImprovementWorkflow.ts",
    "prepublishOnly": "npm run build:all"
  },
  "keywords": [
    "react",
    "ai",
    "workflow",
    "visual",
    "flow",
    "canvas",
    "openai",
    "react-flow",
    "embeddable",
    "component"
  ],
  "author": "Fixter.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blissito/formmy_actions.git"
  },
  "homepage": "https://github.com/blissito/formmy_actions#readme",
  "dependencies": {
    "@ai-sdk/openai": "^2.0.24",
    "@tailwindcss/forms": "^0.5.10",
    "@types/js-yaml": "^4.0.9",
    "@xyflow/react": "^12.8.4",
    "ai": "^5.0.34",
    "js-yaml": "^4.1.0",
    "openai": "^5.19.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "tsx": "^4.20.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
